@model List<InventoryUi.Models.Product>

<table class="table">
    <thead>
        <tr>
            <th>Name</th>
            <th>Quantity</th>
            <th>Price</th>
            <!-- Add other columns as needed -->
        </tr>
    </thead>
    <tbody>
        @foreach (var product in Model)
        {
            <tr>
                <td>@product.ProductName</td>
                <td>@product.Quentity</td>
                <td>@product.TotalPrice</td>
                <!-- Add other columns as needed -->
            </tr>
        }
    </tbody>
</table>
